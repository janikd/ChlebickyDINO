		<div class="slider-goods slider-type-a slider_mod-a owl-carousel owl-theme owl-theme_mod-a enable-owl-carousel"
			 data-pagination="false"
			 data-navigation="true"
			 data-single-item="true"
			 data-auto-play="7000"
			 data-transition-style="fade"
			 data-main-text-animation="true"
			 data-after-init-delay="2000"
			 data-after-move-delay="1000"
			 data-stop-on-hover="true">

			<div class="slider-type-a__item">
				<div class="slider-type-a__img"><img class="img-responsive"
													 src="{$basePath}/resources/front/assets/media/sandwich/6/600.png"
													 height="617" width="618" alt="Foto"></div>
				<div class="slider-type-a__inner">
					<div class="slider-type-a__name">Řepa</div>
					<div class="slider-type-a__price">$18 - $37</div>
					<div class="slider-type-a__description">Marinated Chicken Breast Fillets, Shallots, Roasted
						Capsicum, Caramelised Onions & Bocconcini on a Tomato base, topped with Peri-Peri sauce
					</div>
					<a class="ui-btn ui-btn-primary btn-effect" href="javascript:void(0);">order now</a>
				</div>
			</div>

			<div class="slider-type-a__item">
				<div class="slider-type-a__img"><img class="img-responsive"
													 src="{$basePath}/resources/front/assets/media/sandwich/2/600.png"
													 height="617" width="618" alt="Foto"></div>
				<div class="slider-type-a__inner">
					<div class="slider-type-a__name">Sýr</div>
					<div class="slider-type-a__price">$18 - $37</div>
					<div class="slider-type-a__description">Marinated Chicken Breast Fillets, Shallots, Roasted
						Capsicum, Caramelised Onions & Bocconcini on a Tomato base, topped with Peri-Peri sauce
					</div>
					<a class="ui-btn ui-btn-primary btn-effect" href="javascript:void(0);">order now</a>
				</div>
			</div>

			<div class="slider-type-a__item">
				<div class="slider-type-a__img"><img class="img-responsive"
													 src="{$basePath}/resources/front/assets/media/sandwich/5/600.png"
													 height="617" width="618" alt="Foto"></div>
				<div class="slider-type-a__inner">
					<div class="slider-type-a__name">Vejce</div>
					<div class="slider-type-a__price">$18 - $37</div>
					<div class="slider-type-a__description">Marinated Chicken Breast Fillets, Shallots, Roasted
						Capsicum, Caramelised Onions & Bocconcini on a Tomato base, topped with Peri-Peri sauce
					</div>
					<a class="ui-btn ui-btn-primary btn-effect" href="javascript:void(0);">order now</a>
				</div>
			</div>
		</div>
		<!-- end slider -->
	</div>
	<!-- end container -->
</div>
<!-- end section-bg_mod-c -->

