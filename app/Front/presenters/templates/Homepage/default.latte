{block content}
{*}
<div class="main-slider slider-pro" id="my-slider"
	 data-slider-width="1920"
	 data-slider-height="1080">

	<div class="sp-slides">

		<!-- Slide 1 -->
		<div class="sp-slide">
			<img class="sp-image img-responsive"
				 src="{$basePath}/resources/front/assets/media/main-slider/main-slider_bg-1.jpg" height="1080"
				 width="1920" alt="slider">
			<span class="sp-layer main-slider__title"
				  data-horizontal="0"
				  data-vertical="-23%"
				  data-position="centerCenter"
				  data-show-transition="up"
				  data-hide-transition="down"
				  data-show-duration="800"
				  data-show-delay="400"
				  data-hide-delay="400">
									Čerstvé chlebíčky Dino
								</span>
			<div class="sp-layer main-slider__text text-center"
				 data-horizontal="0"
				 data-vertical="19%"
				 data-position="centerCenter"
				 data-width="45%"
				 data-height="30%"
				 data-show-transition="up"
				 data-hide-transition="down"
				 data-show-duration="800"
				 data-show-delay="1200"
				 data-hide-delay="1200">
				<p class="hidden-xs">Každý den pro Vás chystáme čerstvé a pestré chlebíčky.</p>
				<a class="ui-btn ui-btn_mod-a btn-effect" n:href="Menu:default">objednat online</a>
			</div>
		</div>
		<!-- end sp-slide -->

	</div>
	<!-- end sp-slides -->
</div>
<!-- end main-slider -->
{*}

<div class="main-slider slider-pro"
	 data-slider-width="1920"
	 data-slider-height="1080">
	<div class="board">
		<img src="{$basePath}/resources/front/assets/img/main-slider_bg-1.jpg" alt="">
		<span class="sp-layer main-slider__title" data-horizontal="0"
			  data-vertical="-23%"
			  data-position="centerCenter"
			  data-show-transition="up"
			  data-hide-transition="down"
			  data-show-duration="800"
			  data-show-delay="400"
			  data-hide-delay="400">Čerstvé chlebíčky Dino</span>
		<div class="board-text text-center">
			<p>Každý den pro Vás chystáme čerstvé a pestré chlebíčky.</p>
			<a class="ui-btn ui-btn_mod-a btn-effect" n:href="Menu:default">objednat online</a>
		</div>
	</div>
</div>

<!-- HEADER -->
<header class="header clearfix wow slideInUp" data-wow-duration="2s">
	<div class="header__wrap">
		<span class="sb-toggle-left"><i class="icon pe-7s-menu"></i></span>
		<a class="logo" n:href="Homepage:default">Chlebíčky <img class="img-responsive"
																 src="{$basePath}/resources/front/assets/img/logo_mod-a.png"
																 alt="Logo"></a>
		<span class="header-basket sb-toggle-right"><i class="icon pe-7s-cart"></i><span
					class="header-basket__number">2</span></span>
	</div>
</header>
<!-- end header -->

<section class="section-default">
	<div class="container">
		<div class="row">
			<div class="col-xs-12">
				<h2 class="ui-title-block wow fadeInUp" data-wow-duration="2s">Delame chlebicky.</h2>
				<p class="ui-subtitle-block wow fadeInUp" data-wow-duration="2s">{$lorem}</p>

				<div class="advantages">
					<div class="row">
						<div class="col-md-4">
							<div class="advantages__item wow fadeInLeft" data-wow-duration="2s">
								<i class="icon pe-7s-leaf"></i>
								<h3 class="advantages__title ui-title-inner">Zdravé</h3>
								<div class="advantages__description">{$lorem}</div>
							</div>
						</div>

						<div class="col-md-4">
							<div class="advantages__item wow fadeInUp" data-wow-duration="2s">
								<i class="icon pe-7s-medal"></i>
								<h3 class="advantages__title ui-title-inner">Chutné</h3>
								<div class="advantages__description">{$lorem}</div>
							</div>
						</div>

						<div class="col-md-4">
							<div class="advantages__item wow fadeInRight" data-wow-duration="2s">
								<i class="icon pe-7s-rocket"></i>
								<h3 class="advantages__title ui-title-inner">Nápadité</h3>
								<div class="advantages__description">{$lorem}</div>
							</div>
						</div>
					</div>
				</div>
				<!-- end advantages -->
			</div>
			<!-- end col -->
		</div>
		<!-- end row -->
	</div>
	<!-- end container -->
</section>
<!-- end section-default -->


