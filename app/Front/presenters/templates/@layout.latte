<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimal-ui">

	<link href="{$basePath}/favicon.png?v=2" type="image/png" rel="shortcut icon">
	<link href="{$basePath}/resources/front/assets/css/master.css" rel="stylesheet">
	<script src="{$basePath}/resources/front/assets/plugins/jquery/jquery-1.11.3.min.js"></script>
	<link href="https://fonts.googleapis.com/css?family=Rakkas" rel="stylesheet">

	<title>{ifset title}{include title|stripHtml} | {/ifset}Chlebíčky DINO</title>
	{ifset styles}{include styles}{/ifset}
</head>

<body>
<div n:foreach="$flashes as $flash" n:class="flash, $flash->type">{$flash->message}</div>

<!-- Loader -->
<div id="page-preloader"><span class="spinner"></span></div>
<!-- Loader end -->
<div class="layout-theme animated-css" id="wrapper" data-header="sticky" data-header-top="700">

	<div id="sb-site">

		<div class="wrap-content">

			{if !$presenter->isLinkCurrent('Homepage:default')}
				<!-- HEADER -->
				<header class="header clearfix">
					<div class="header__wrap">
						<span class="sb-toggle-left"><i class="icon pe-7s-menu"></i></span>
						<a class="logo" n:href="Homepage:default">Chlebíčky <img class="img-responsive"
																				 src="{$basePath}/resources/front/assets/img/logo_mod-a.png"
																				 alt="Logo"></a>
						<span class="header-basket sb-toggle-right"><i class="icon pe-7s-cart"></i><span
									class="header-basket__number">2</span></span>
					</div>
				</header>
				<!-- end header -->
			{/if}

			{include content}

			<!-- footer -->
			{include '_footer.latte'}
			<!-- end footer -->

		</div>
		<!-- end wrap-content -->

	</div>
	<!-- end #sb-site -->

	{include '_menu.latte'}

	<div class="sb-slidebar sb-right">
		<section class="section-list-cart">
			<h2 class="ui-title-inner">Rychlý košík</h2>
			<ul class="list-cart list-unstyled">
				<li class="list-cart__item">
					<div class="list-cart__img"><i class="list-cart__icon icon fa fa-times js-del"></i><img
								class="img-responsive" src="{$basePath}/resources/front/assets/media/sandwich/2/600.png"
								height="87" width="87"
								alt="Foto"></div>
					<div class="list-cart__inner">
						<h3 class="list-cart__title">Moorish Lamb</h3>
						<div class="list-cart__size"><span class="list-cart__size_name">Size:</span> Large</div>
						<div class="list-cart__price"><span class="color_primary">1 x</span> $19.00</div>
					</div>
				</li>
				<li class="list-cart__item">
					<div class="list-cart__img"><i class="list-cart__icon icon fa fa-times js-del"></i><img
								class="img-responsive" src="{$basePath}/resources/front/assets/media/sandwich/4/600.png"
								height="87" width="87"
								alt="Foto"></div>
					<div class="list-cart__inner">
						<h3 class="list-cart__title">Vegetarian Supreme</h3>
						<div class="list-cart__size"><span class="list-cart__size_name">Size:</span> Jumbo</div>
						<div class="list-cart__price"><span class="color_primary">1 x</span> $34.00</div>
					</div>
				</li>
				<li class="list-cart__item">
					<div class="list-cart__img"><i class="list-cart__icon icon fa fa-times js-del"></i><img
								class="img-responsive" src="{$basePath}/resources/front/assets/media/sandwich/6/600.png"
								height="87" width="87"
								alt="Foto"></div>
					<div class="list-cart__inner">
						<h3 class="list-cart__title">Spiced Pumpkin</h3>
						<div class="list-cart__size"><span class="list-cart__size_name">Size:</span> Small</div>
						<div class="list-cart__price"><span class="color_primary">1 x</span> $26.00</div>
					</div>
				</li>
			</ul>
			<div class="total-price clearfix">Celkem
				<span class="total-price__number">$79.00</span>
			</div>
			<a class="total-price__btn ui-btn ui-btn_mod-a btn-effect btn-block" n:href="Cart:default">zobrazit
				košík</a>
			<a class="total-price__btn ui-btn ui-btn-primary btn-effect btn-block" n:href="Cart:checkout">objednat</a>
		</section>
		<!-- end list-cart -->
	</div>
	<!-- end sb-right -->

</div>
<!-- end layout-theme -->

<!-- SCRIPTS MAIN -->
<script src="{$basePath}/resources/front/assets/js/jquery-migrate-1.2.1.js"></script>
<script src="{$basePath}/resources/front/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
<script src="{$basePath}/resources/front/assets/js/waypoints.min.js"></script>
<script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
<script src="{$basePath}/resources/front/assets/js/modernizr.custom.js"></script>
<script src="{$basePath}/resources/front/assets/js/cssua.min.js"></script>
<script src="https://nette.github.io/resources/js/netteForms.min.js"></script>

<!--SCRIPTS THEME-->

<!-- Sidebar -->
<script src="{$basePath}/resources/front/assets/plugins/slidebars/dist/slidebars.js"></script>
<!-- Home slider -->
<script src="{$basePath}/resources/front/assets/plugins/slider-pro/dist/js/jquery.sliderPro.js"></script>
<!-- Sliders -->
<script src="{$basePath}/resources/front/assets/plugins/owl-carousel/owl.carousel.min.js"></script>
<!-- Isotope -->
<script src="{$basePath}/resources/front/assets/plugins/isotope/isotope.pkgd.min.js"></script>
<!-- Modal -->
<script src="{$basePath}/resources/front/assets/plugins/prettyphoto/js/jquery.prettyPhoto.js"></script>
<!-- Date select -->
<script src="{$basePath}/resources/front/assets/plugins/datetimepicker/jquery.datetimepicker.js"></script>
<!-- Select customization -->
<script src="{$basePath}/resources/front/assets/plugins/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
<!-- Price slider -->
<script src="{$basePath}/resources/front/assets/plugins/nouislider/jquery.nouislider.all.min.js"></script>
<!-- Chart -->
<script src="{$basePath}/resources/front/assets/plugins/rendro-easy-pie-chart/dist/jquery.easypiechart.min.js"></script>
<!-- Animation -->
<script src="{$basePath}/resources/front/assets/js/wow.min.js"></script>
<!-- Scrollspy -->
<script src="{$basePath}/resources/front/assets/js/scrollspy.min.js"></script>

<!-- Custom -->
<script src="{$basePath}/resources/front/assets/js/custom.js"></script>


{block scripts}
{/block}
</body>
</html>
